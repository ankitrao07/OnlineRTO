<div class="row">
    
</div>
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title"><i class="fa fa-table fa-fw"></i> Edit Registrations</h3>
            </div>
            <div class="panel-body">
                <div class="table-responsive">
                    <table class="table table-striped table-bordered table-hover" id="tblDashBoard" cellspacing="0" align="center" style="width:100%;" ng-cloak>
                        <tr>
                            <td><label class="col-md-4">Vehicle Owner:</label></td>
                            <td><input type="text" id="txtOwner" class="col-md-8" data-ng-model="Details.Owner" placeholder="Vehicle Owner" /></td>
                        </tr>
                        <tr>
                            <td><label class="col-md-4">Mobile:</label></td>
                            <td><input type="text" id="txtMobile" class="col-md-8" data-ng-model="Details.Mobile" placeholder="Mobile" /></td>
                        </tr>
                        <tr>
                            <td><label class="col-md-4">Email</label></td>
                            <td><input type="text" id="txtEmail" class="col-md-8" data-ng-model="Details.Email" placeholder="Email" /></td>
                        </tr>
                        <tr>
                            <td><label class="col-md-4">Address:</label></td>
                            <td><input type="text" id="txtAddress" class="col-md-8" data-ng-model="Details.Address" placeholder="Engine capacity" /></td>
                        </tr>
                        <tr>
                            <td><label class="col-md-4">Manufacture:</label></td>
                            <td><input type="text" id="txtManufacturer" class="col-md-8" data-ng-model="Details.Manufacturer" placeholder="Manufacturer" /></td>
                        </tr>
                        <tr>
                            <td><label class="col-md-4">Vehicle:</label></td>
                            <td><input type="text" id="txtVehicle" class="col-md-8" data-ng-model="Details.Vehicle" placeholder="Vehicle Name" /></td>
                        </tr>
                        <tr>
                            <td><label class="col-md-4">Chasis No:</label></td>
                            <td><input type="text" class="col-md-8" data-ng-model="Details.ChasisNo" placeholder="Chasis No" /></td>
                        </tr>
                        <tr>
                            <td><label class="col-md-4">Color:</label></td>
                            <td><input type="text" id="txtColor" class="col-md-8" data-ng-model="Details.color" placeholder="Color" /></td>
                        </tr>
                        <tr>
                            <td><label class="col-md-4">Engine(cc):</label></td>
                            <td><input type="text" id="txtEngine" class="col-md-8" data-ng-model="Details.Engine" placeholder="Engine capacity" /></td>
                        </tr>
                    </table>
                    <div class="form-group col-md-10">
                        <button type="submit" name="btnApprove" class="col-md-offset-5 col-md-2 btn btn-primary" data-ng-click="save();" value="Update"><i class="fa fa-check fa-fw"></i>Update</button>
                        <a href="showregistration" class="btn btn-primary col-md-offset-1 col-md-2"><i class="fa fa-fw fa-backward"></i>Cancel</a>
                    </div>
                </div>
            </div>
        </div>
    </div> 
    </div>
