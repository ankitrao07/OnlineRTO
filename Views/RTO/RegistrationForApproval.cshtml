<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title"><i class="fa fa-money fa-fw"></i> Pending Registrations</h3>
            </div>
            <div class="panel-body">
                <div class="table-responsive">
                    <table class="table table-striped table-bordered table-hover" id="tblDashBoard" cellspacing="0" align="center" style="width:100%;" ng-cloak>
                        <thead>
                            <tr>
                                <th style="display: none;">RegId</th>
                                <th>TempRegistration</th>
                                <th>Manufacturer</th>
                                <th>Vehicle</th>
                                <th>OwnerName</th>
                                <th>MobileNo</th>
                                <th>Actions</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="items in RegistrationData">
                                <td hidden="hidden">{{items.RegId}}</td>
                                <td>{{items.TempRegistrationNo}}</td>
                                <td>{{items.Manufacturer}}</td>
                                <td>{{items.Vehicle}}</td>
                                <td>{{items.Owner}}</td>
                                <td>{{items.Mobile}}</td>
                                <td>
                                    <a class="btn btn-primary" ng-click="Approve(items.RegId)"><i class="fa fa-check fa-fw"  aria-hidden="true"></i>Approve</a>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>


